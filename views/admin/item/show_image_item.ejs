<div class="tab-pane fade show active" id="show-image" role="tabpanel" aria-labelledby="home-tab">
    <!-- DataTales Example -->
    <div class="row">
        <% for(let j = 0; j < item.imageId.length; j++) { %>
            <div class="col-md-4">
                <div class="card shadow mb-4 mt-2">
                    <div class="card-body">
                        <img src="https://admin-staycation-1818.herokuapp.com/<%= item.imageId[j].imageUrl %>" alt="image" style="width:100%; display:block">
                    </div>
                </div>
            </div>
        <% } %>  
    </div>
    <a href="/admin/item" class="btn btn-warning">Back</a>
</div>